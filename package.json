    "tooltip.js": "^1.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.0.37",
    "@types/jquery": "^3.2.15",
    "@types/node": "^8.0.46",
    "@types/react": "^16.0.18",
    "@types/react-dom": "^16.0.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "cross-env": "^5.1.0",
