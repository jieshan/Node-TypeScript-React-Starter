  "scripts": {
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "start": "nodemon dist/backend/server.js",
    "build": "tsc && ./node_modules/.bin/webpack --devtool source-map && node copyStaticAssets.js",
